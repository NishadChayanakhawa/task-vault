<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Test Estimate Hub UI Tests" parallel="tests" thread-count="5">
	<listeners>
		<listener
			class-name="io.nishadc.automationtestingframework.testngcustomization.ReportGenerator"></listener>
	</listeners>
	<test name="Task type configuration tests">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.taskvault.tests.ui.TaskTypeConfigurationTests" />
		</classes>
	</test>
	<test name="Task group configuration tests">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.taskvault.tests.ui.TaskGroupConfigurationTests" />
		</classes>
	</test>
	<test name="Resource configuration tests">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.taskvault.tests.ui.ResourceConfigurationTests" />
		</classes>
	</test>
</suite>